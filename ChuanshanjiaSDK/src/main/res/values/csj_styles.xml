<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="yw_AppTheme" parent="android:Theme.Light" />

    <style name="yw_LoginDialog" parent="@android:style/Theme.Dialog">

        <!-- name是我们在使用时要用到的资源的标志，parent是指当前的样式所继承的父类样式 -->
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <!-- <item name="android:background">@drawable/login_bg</item> -->
        <item name="android:windowBackground">@drawable/csj_progressdialog_bg</item>
        <!-- 最底 边框图 <item name="android:windowBackground">@android:color/transparent</item> -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 屏幕背景是否变暗 -->
        <item name="android:backgroundDimAmount">0</item>
    </style>

    <!-- 编辑框 -->
    <style name="csj_option_edittext_style">
        <item name="android:textSize">16sp</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">#FF000000</item>
    </style>


    <!-- 外框 -->
    <style name="csj_option_margin_style">
        <item name="android:layout_marginTop">0dip</item>
        <item name="android:layout_marginBottom">10dip</item>
        <item name="android:layout_marginLeft">0dip</item>
        <item name="android:layout_marginRight">0dip</item>
    </style>
    <dimen name="csj_option_item_marginLeft">10dip</dimen>
    <dimen name="csj_register_agreement_text_size">13sp</dimen>


    <style name="csj_button_text_style">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#FFFFFFFF</item>
    </style>


    <style name="csj_corner_text_style">
        <item name="android:textSize">20sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/csj_gray</item>
        <item name="android:layout_marginTop">10dip</item>
    </style>


    
     <style name="MyDialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- <item name="android:windowBackground">@drawable/fragment_dialog_bg</item> -->
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="loading_dialog" >

        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>


    </style>
    <style name="Photo_dialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item> 
        <item name="android:windowBackground">@drawable/csj_dialog_bg</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="transcutestyle_yhxf" parent="@android:style/Theme.Dialog">
         <item name="android:windowFrame">@android:color/transparent</item><!--边框-->
         <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
         <item name="android:windowIsTranslucent">true</item><!--半透明-->
         <item name="android:windowNoTitle">true</item><!--无标题-->
         <item name="android:windowBackground">@android:color/transparent</item><!--背景透明-->
         <item name="android:backgroundDimAmount">0.3</item>
        <item name="android:windowAnimationStyle">@null</item>
     </style>
         <style name="question_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
     </style>
      <style name="extra_item_content_style">
        <item name="android:textSize">@dimen/pp_font_size_normal</item>
        <item name="android:textColor">@color/sliding</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>
     <style name="dialog" parent="@android:style/Theme.Dialog">

        <!-- name是我们在使用时要用到的资源的标志，parent是指当前的样式所继承的父类样式 -->
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <!-- <item name="android:background">@drawable/login_bg</item> -->
        <!-- 最底 边框图 <item name="android:windowBackground">@android:color/transparent</item> -->
        <item name="android:backgroundDimEnabled">true</item>
    </style>
      <style name="footbar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

</resources>